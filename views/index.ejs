<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title>Covid 19</title>
</head>
<body>
    <div class="background">
        <% stories.forEach(function(story){ %>
            <div class="cases-container">
                <h1><%= story.Combined_Key %></h1>
                    <p>Confirmed Cases: <%= story.Confirmed %></p> 
                    <p>Active Cases: <%= story.Active %></p> 
                    <p>Recoveries: <%= story.Recovered %></p> 
                    <p>No. of Deaths: <%= story.Deaths %></p>   
            </div>
        <% }); %>
        </div>

</body>
</html>
